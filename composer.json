{
    "name": "frowhy/modular",
    "description": "Modular development in laravel.",
    "type": "library",
    "require": {
        "php": "^7.2",
        "nwidart/laravel-modules": "^5.0",
        "frowhy/module-core": "^0.1"
    },
    "require-dev": {
        "laravel/framework": "^5.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Govern Fu",
            "email": "frowhy@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Frowhy\\Modular\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Frowhy\\Modular\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
